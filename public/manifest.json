{
  "name": "English Syntax Highlighter",
  "version": "1.0",
  "description": "Highlight english words",
  "permissions": [
    "tabs",
    "storage",
    "*://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "English Syntax Highlighter",
    "default_popup": "popup.html"
  },
  "options_page": "index.html",
  "manifest_version": 2
}